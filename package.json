{
  "name": "ngx-remote-config",
  "version": "1.0.0",
  "description": "Remote configurations for Angular8+ applications, with built-in interceptor for mock REST data and non-permanent api",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "remote",
    "config",
    "settings",
    "mock",
    "rest",
    "api",
    "directives",
    "angular8",
    "ng8"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=11",
    "npm": ">=6.5.0"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-remote-config/issues"
  },
  "homepage": "https://endykaufman.github.io/ngx-remote-config/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-remote-config.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-remote-config --prod",
    "lib:postbuild-lib": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/ngx-remote-config/CHANGELOG.md",
    "lib:build": "run-s prepare:prod lib:build-lib lib:postbuild-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s format:fix prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve -- --host=0.0.0.0  --disable-host-check",
    "start": "run-s format:fix prepare:mono serve",
    "build": "run-s app:build",
    "test": "run-s github:build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-remote-config/ --prod",
    "github:build": "run-s format:fix lib:build config:prod github:prebuild github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo",
    "format:fix": "prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^8.2.13",
    "@angular/http": "^7.2.15",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/pwa": "^0.803.17",
    "@angular/router": "^8.2.13",
    "@angular/service-worker": "^8.2.13",
    "@ngx-prism/core": "^2.0.1",
    "@types/dot-object": "^1.7.0",
    "@types/lodash-es": "^4.17.3",
    "@types/prismjs": "^1.16.0",
    "bind-observable": "^1.0.3",
    "core-js": "^2.6.3",
    "dot-object": "^2.1.2",
    "lodash-es": "^4.17.15",
    "ngx-markdown": "^8.2.1",
    "path-to-regexp": "^3.1.0",
    "prismjs": "^1.17.1",
    "raw-loader": "^0.5.1",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular-devkit/build-ng-packagr": "~0.803.17",
    "@angular/cli": "^8.3.17",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@rucken/cli": "^4.1.5",
    "@types/jasmine": "~3.4.5",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.12.5",
    "codelyzer": "~5.2.0",
    "conventional-changelog-cli": "^2.0.25",
    "conventional-commits-detector": "^1.0.2",
    "conventional-recommended-bump": "^6.0.2",
    "cp-cli": "^2.0.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.5.4",
    "markdown-loader": "^5.1.0",
    "ng-packagr": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "core-js",
      "raw-loader",
      "rxjs",
      "zone.js"
    ]
  }
}
